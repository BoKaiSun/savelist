class savelist(object):
	def __init__(self):
		pass
		# self.r = r      # public
		# self.l = l   # private

	def write(data):
		r=""
		for i in data:
			r+=str(i)+"\n"
		return r.strip()

	def read(data):
		if type(data) is str:
			l=[]
			for i in data.strip().split("\n"):
				l.append(i)
			return l
		
		elif type(data) is list:
			l=[]
			for i in data:
				l.append(i.strip())
			return l
